def build(bld):
    obj = bld.create_obj('cc', 'staticlib')
    obj.source = "coll.c value.c xlist.c".split()
    obj.includes='. ../../include ../../includepriv'
    obj.target='xmmstypes'
    obj.install_var = 0

def configure(conf):
    return True

def set_options(opt):
    pass
